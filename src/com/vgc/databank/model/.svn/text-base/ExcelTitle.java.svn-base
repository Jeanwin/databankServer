package com.vgc.databank.model;
/**
 * record title, title description and so on
 * 
 * @author bull
 *
 */
public class ExcelTitle {
	/**
	 * title key
	 * corresponding conf file genExcelConf.xml text of element key
	 */
	private String titleKey;
	/**
	 * title description
	 * corresponding conf file genExcelConf.xml text of element desc
	 */
	private String titleDesc;
	/**
	 * obtain title key
	 * @return
	 */
	public String getTitleKey() {
		return titleKey;
	}
	
	public void setTitleKey(String titleKey) {
		this.titleKey = titleKey;
	}
	/**
	 * obtain title description
	 * @return
	 */
	public String getTitleDesc() {
		return titleDesc;
	}

	public void setTitleDesc(String titleDesc) {
		this.titleDesc = titleDesc;
	}
	/**
	 * obtain hold column number
	 */
	public int getHoldColumnNum() {
		return holdColumnNum;
	}

	public void setHoldColumnNum(int holdColumnNum) {
		this.holdColumnNum = holdColumnNum;
	}

	private int holdColumnNum;
	
	
}
